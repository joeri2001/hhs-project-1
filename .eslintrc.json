{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "prettier" // Ensure Prettier is the last in the array
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "indent": ["error", 2]
  }
}
